<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.sicnu.dao.LogoperationDao">

    <resultMap type="cn.edu.sicnu.entity.Logoperation" id="LogoperationMap">
        <result property="logtableId" column="logtable_id" jdbcType="INTEGER"/>
        <result property="logcolumnId" column="logcolumn_id" jdbcType="INTEGER"/>
        <result property="optionType" column="option_type" jdbcType="VARCHAR"/>
        <result property="userId" column="user_id" jdbcType="VARCHAR"/>
    </resultMap>


    <!--新增所有列-->
    <insert id="insert">
        insert into scientific_research.logoperation (logtable_id,logcolumn_id,option_type, user_id)
        values (#{logtableId},#{logcolumnId},#{optionType}, #{userId})
    </insert>


</mapper>
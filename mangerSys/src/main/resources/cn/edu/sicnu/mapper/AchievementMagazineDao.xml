<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.sicnu.dao.AchievementMagazineDao">

    <resultMap type="cn.edu.sicnu.entity.AchievementMagazine" id="AchievementMagazineMap">
        <result property="achievementId" column="achievement_id" jdbcType="VARCHAR"/>
        <result property="magazineId" column="magazine_id" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="AchievementMagazineMap">
        select magazine_id
        from scientific_research.achievement_magazine
        where achievement_id = #{achievementId}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="AchievementMagazineMap">
        select achievement_id, magazine_id
        from scientific_research.achievement_magazine
                     limit #{offset}, #{limit}
    </select>
    <!--查询表所有对象-->
    <select id="findAll" resultMap="AchievementMagazineMap">
        select achievement_id, magazine_id
        from scientific_research.achievement_magazine
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="AchievementMagazineMap">
        select
        achievement_id, magazine_id
        from scientific_research.achievement_magazine
        <where>
            <if test="achievementId != null and achievementId != ''">
                and achievement_id = #{achievementId}
            </if>
            <if test="magazineId != null and magazineId != ''">
                and magazine_id = #{magazineId}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="achievementId" useGeneratedKeys="true">
        insert into scientific_research.achievement_magazine (magazine_id)
        values (#{magazineId})
    </insert>

    <insert id="insertBatch" keyProperty="achievementId" useGeneratedKeys="true">
        insert into scientific_research.achievement_magazine(magazine_id)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.magazineId})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="achievementId" useGeneratedKeys="true">
        insert into scientific_research.achievement_magazine(magazine_id)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.magazineId})
        </foreach>
        on duplicate key update
        magazine_id = values(magazine_id)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update scientific_research.achievement_magazine
        <set>
            <if test="magazineId != null and magazineId != ''">
                magazine_id = #{magazineId},
            </if>
        </set>
        where achievement_id = #{achievementId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete
        from scientific_research.achievement_magazine
        where achievement_id = #{achievementId}
    </delete>

</mapper>